{
    class defconstructor
    {
        defconstructor()
        {
            Console.WriteLine("Cunstructor called.");
        }
      ~defdistructor()
        {
           Console.WriteLine(“Destructor called”);
           Console.ReadLine();
        }
        static void Main(string[] args)
        {
            defconstructor d1 = new defconstructor();
            
        }
    }
